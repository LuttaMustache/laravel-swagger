{
  "name": "luttamustache/op-laravel-swagger",
  "description": "Fork from ronasit/laravel-swagger for old php versions (<=7.2).",
  "keywords": ["laravel", "swagger", "testing", "auto-documentation", "rest-api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Olzhas Zhangeldinov",
      "email": "olzhas.290100@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "laravel/framework": ">=5.3.0",
    "minime/annotations": "2.3.1",
    "ronasit/laravel-helpers": "1.2.*",
    "ronasit/local-data-collector": "1.1.*@beta"
  },
  "autoload": {
    "psr-4": {
      "LuttaMustache\\Support\\AutoDoc\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  }
}
