{
  "name": "luttamustache/op-laravel-swagger",
  "description": "Provided middleware for generating of swagger-documentation file by run testing of RESTful API.",
  "keywords": [
    "laravel",
    "swagger",
    "testing",
    "auto-documentation",
    "rest-api"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Dubrovin",
      "email": "rdubrovin@ronasit.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "laravel/framework": "8.*",
    "minime/annotations": "dev-master",
    "nikic/php-parser": "4.10.4"
  },
  "autoload": {
    "psr-4": {
      "LuttaMustache\\Support\\AutoDoc\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Tests/"
    ]
  }
}
